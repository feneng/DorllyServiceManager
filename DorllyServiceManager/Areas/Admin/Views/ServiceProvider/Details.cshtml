@model DorllyService.Domain.ServiceSupplier
@{
    Layout = null;
    ViewBag.Title = "服务商信息";
}

<div class="row">
    <div class="col-md-12">
        <div class="form-title">
            @(ViewBag.PageCode)基本信息
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="Code"></label>
                <input type="text" class="form-control" asp-for="Code" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="Avatar"></label>
                <input type="file" class="form-control" asp-for="Avatar" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="BelongGardenId"></label>
                <input type="text" class="form-control" asp-for="BelongGarden.Name" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="ChargePerson"></label>
                <input type="text" class="form-control" asp-for="ChargePerson" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="FullName"></label>
                <input type="text" class="form-control" asp-for="FullName" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="Abbreviation"></label>
                <input type="text" class="form-control" asp-for="Abbreviation" readonly="readonly">
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="ServiceScope"></label>
                <input type="text" class="form-control" asp-for="ServiceScope" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="ContactPhone"></label>
                <input type="text" class="form-control" asp-for="ContactPhone" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="Email"></label>
                <input type="email" class="form-control" asp-for="Email" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="WorkTel"></label>
                <input type="text" class="form-control" asp-for="WorkTel" readonly="readonly">
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <label asp-for="SupplierFrom"></label>
                <input type="text" class="form-control" asp-for="SupplierFrom" readonly="readonly">
            </div>
        </div>
    </div>
</div>
