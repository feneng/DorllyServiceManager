@model DorllyService.Domain.Module
@{
    Layout = null;
    ViewBag.PageCode = "菜单";
    ViewBag.Title = $"删除{ViewBag.PageCode}";
}

<div class="row">
    <div class="col-md-12">
        <form role="form" method="post" asp-area="Admin" asp-controller="Module" asp-action="Delete" id="deleteform">
            <input asp-for="Id" type="hidden" />
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Name"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="Name">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Type"></label>
                    <input type="text" disabled="disabled" class="form-control" asp-for="Type">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="ParentId"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="ParentId">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="BelongSystemId"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="BelongSystemId">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Level"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="Level">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Order"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="Order">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Path"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="Path">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Status"></label>
                    <select asp-for="Status" class="form-control" disabled="disabled">
                        <option value="True">启用</option>
                        <option value="False">禁用</option>
                    </select>
                </div>
            </div>
        </form>
    </div>
</div>