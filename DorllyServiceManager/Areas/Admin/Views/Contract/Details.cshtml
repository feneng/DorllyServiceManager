@model DorllyService.Domain.Contract

@{
    ViewBag.Title = "合同信息";
}

<h1>合同详情</h1>

<div>
    <h4>@Model.Name</h4>
    <hr />
    <dl class="row">

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ContractNo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ContractNo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.State)
        </dt>
        <dd class="col-sm-10">
