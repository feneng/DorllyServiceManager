@model DorllyService.Domain.ServiceCategory
@{
    Layout = null;
    ViewBag.PageCode = "服务分类";
    ViewBag.Title = $"删除{ViewBag.PageCode}";
}

<div class="row">
    <div class="col-md-12">
        <form role="form" method="post" asp-area="Admin" asp-controller="ServiceCategory" asp-action="Delete" id="deleteform">
            <input asp-for="Id" type="hidden" />
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Name"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="Name">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Code"></label>
                    <input type="text" disabled="disabled" class="form-control" asp-for="Code">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="ParentId"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="ParentId">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Parent"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="Parent.Name">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Level"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="Level">
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Status"></label>
                    <input type="text" class="form-control" disabled="disabled" asp-for="Status">
                </div>
            </div>
        </form>
    </div>
</div>