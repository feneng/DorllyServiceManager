@model DorllyService.Domain.ServiceProperty
@{
    Layout = null;
    ViewBag.PageCode = "服务属性";
    ViewBag.Title = $"删除{ViewBag.PageCode}";
}

<div class="row">
    <div class="col-md-12">
        <form role="form" method="post" asp-area="Admin" asp-controller="ServiceProperty" asp-action="Delete" id="deleteform">
            <input asp-for="Id" type="hidden" />
            <div class="form-title">
                @(ViewBag.PageCode)基本信息
            </div>

            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Code"></label>
                    <input type="text" class="form-control" asp-for="Code" readonly="readonly">
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Name"></label>
                    <input type="text" class="form-control" asp-for="Name" readonly="readonly">
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Type"></label>
                    <input type="text" class="form-control" asp-for="TypeName" readonly="readonly">
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="InputType"></label>
                    <input type="text" class="form-control" asp-for="InputTypeName" readonly="readonly">
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="State"></label>
                    <input type="text" class="form-control" asp-for="StateName" readonly="readonly">
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <div>
                        <label asp-for="NotNull"></label>
                        <input type="text" class="form-control" value='@(Model.NotNull?"是":"否")' readonly="readonly">
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
