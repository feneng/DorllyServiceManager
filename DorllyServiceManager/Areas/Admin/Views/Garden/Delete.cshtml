@model DorllyService.Domain.Garden
@{
    Layout = null;
    ViewBag.PageCode = "园区";
    ViewBag.Title = $"删除{ViewBag.PageCode}";
}

<div class="row">
    <div class="col-md-12">
        <form role="form" method="post" asp-area="Admin" asp-controller="Garden" asp-action="Delete" id="deleteform">
            <input asp-for="Id" type="hidden" />
            <div class="form-title">
                @(ViewBag.PageCode)基本信息
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Name"></label>
                    <input type="text" class="form-control" asp-for="Name" readonly="readonly" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <label asp-for="Address"></label>
                    <input type="text" class="form-control" asp-for="Address" readonly="readonly" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-12 col-sm-12 col-xs-12">
                    <label asp-for="State"></label>
                    <input type="text" class="form-control" asp-for="StateName" readonly="readonly" />
                 </div>
            </div>
        </form>
    </div>
</div>
